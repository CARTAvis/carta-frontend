{
    "name": "carta-frontend",
    "version": "1.1.0-rc.0",
    "private": true,
    "homepage": "./",
    "dependencies": {
        "@blueprintjs/core": "^3.15.0",
        "@blueprintjs/icons": "^3.7.0",
        "@blueprintjs/select": "^3.8.0",
        "chart.js": "^2.8.0",
        "chartjs-plugin-annotation": "^0.5.7",
        "golden-layout": "^1.5.9",
        "konva": "^3.2.3",
        "lodash": "^4.17.11",
        "mobx": "^5.9.4",
        "mobx-react": "^5.4.3",
        "raw-loader": "^0.5.1",
        "react": "^16.8.6",
        "react-chartjs-2": "^2.7.6",
        "react-color": "^2.17.0",
        "react-dom": "^16.8.6",
        "react-flexbox-grid": "^2.1.2",
        "react-konva": "^16.8.6",
        "react-resize-detector": "^4.0.5",
        "react-rnd": "^9.1.2",
        "react-scripts-ts": "^2.17.0",
        "react-scroll-to-bottom": "^1.3.1",
        "react-transition-group": "^2.8.0",
        "rxjs": "^6.4.0",
        "source-map-explorer": "^1.8.0",
        "ws": "^6.2.1"
    },
    "scripts": {
        "git-info": "echo export default {logMessage: \\\"$(git log -1 --oneline)\\\"}\\;  > src/static/gitInfo.ts",
        "analyze": "source-map-explorer build/static/js/main.*",
        "build-css": "node-sass-chokidar -q src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar -q src/ -o src/ --watch --recursive",
        "build-wrappers": "./wasm_src/build_wrappers.sh",
        "build-protobuf": "./protobuf/build_proto.sh",
        "start-js": "react-scripts-ts start",
        "start": "npm run git-info; npm-run-all -p watch-css start-js",
        "build-js": "react-scripts-ts build",
        "build": "npm run git-info; npm-run-all build-wrappers build-protobuf build-css build-js",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject"
    },
    "devDependencies": {
        "@types/chart.js": "^2.7.50",
        "@types/jest": "^24.0.11",
        "@types/jquery": "^3.3.28",
        "@types/lodash": "^4.14.123",
        "@types/node": "^11.13.0",
        "@types/react": "^16.8.12",
        "@types/react-color": "^3.0.0",
        "@types/react-dom": "^16.8.3",
        "@types/react-resize-detector": "^3.1.1",
        "chokidar-cli": "^1.2.2",
        "mobx-react-devtools": "^6.1.1",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.3",
        "protobufjs": "^6.8.8",
        "typescript": "^3.4.1",
        "worker-loader": "^2.0.0"
    }
}
