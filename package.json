{
    "name": "carta-frontend",
    "version": "1.2.0",
    "private": true,
    "homepage": "./",
    "dependencies": {
        "@blueprintjs/core": "^3.18.1",
        "@blueprintjs/icons": "^3.10.0",
        "@blueprintjs/select": "^3.10.0",
        "carta-ast-wrapper": "file:wasm_src/ast_wrapper/build",
        "carta-protobuf": "file:./protobuf/build",
        "carta-zfp-wrapper": "file:./wasm_src/zfp_wrapper/build",
        "chart.js": "^2.8.0",
        "chartjs-plugin-annotation": "^0.5.7",
        "golden-layout": "^1.5.9",
        "konva": "^4.0.10",
        "lodash": "^4.17.15",
        "mnemonist": "^0.30.0",
        "mobx": "^5.13.0",
        "mobx-react": "^6.1.3",
        "raw-loader": "^0.5.1",
        "react": "^16.9.0",
        "react-chartjs-2": "^2.7.6",
        "react-color": "^2.17.3",
        "react-dom": "^16.9.0",
        "react-flexbox-grid": "^2.1.2",
        "react-konva": "^16.9.0-0",
        "react-resize-detector": "^4.2.0",
        "react-rnd": "^10.0.0",
        "react-scripts-ts": "^3.1.0",
        "react-scroll-to-bottom": "^1.3.2",
        "rxjs": "^6.5.2",
        "source-map-explorer": "^2.0.1"
    },
    "scripts": {
        "git-info": "echo export default {logMessage: \\\"$(git log -1 --oneline)\\\"}\\;  > src/static/gitInfo.ts",
        "analyze": "source-map-explorer build/static/js/main.*",
        "build-css": "node-sass-chokidar -q src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar -q src/ -o src/ --watch --recursive",
        "build-wrappers": "./wasm_src/build_wrappers.sh",
        "build-protobuf": "./protobuf/build_proto.sh",
        "start-js": "react-scripts-ts start",
        "start": "npm run git-info; npm-run-all -p watch-css start-js",
        "build-js": "react-scripts-ts build",
        "build": "npm-run-all git-info build-wrappers build-protobuf build-css build-js",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject"
    },
    "devDependencies": {
        "@types/chart.js": "^2.8.5",
        "@types/jest": "^24.0.18",
        "@types/jquery": "^3.3.31",
        "@types/lodash": "^4.14.138",
        "@types/node": "^12.7.5",
        "@types/react": "^16.9.2",
        "@types/react-color": "^3.0.1",
        "@types/react-dom": "^16.9.0",
        "@types/react-resize-detector": "^4.0.2",
        "chokidar-cli": "^2.0.0",
        "node-sass-chokidar": "^1.3.5",
        "npm-run-all": "^4.1.3",
        "protobufjs": "^6.8.8",
        "typescript": "^3.6.3",
        "worker-loader": "^2.0.0"
    }
}
